+#ifdef CONFIG_BOARD_GRASSHOPPER_PWM0
+			| (1 << 0) 
+#endif
+#ifdef CONFIG_BOARD_GRASSHOPPER_PWM1
+			| (1 << 1) 
+#endif
+#ifdef CONFIG_BOARD_GRASSHOPPER_PWM2
+			| (1 << 2) 
+#endif
+#ifdef CONFIG_BOARD_GRASSHOPPER_PWM3
+			| (1 << 3) 
+#endif
+			);
+	platform_device_register(&pwm_led_dev);
+	//grasshopper_setup_pwm(2,25000000); // 25MHz
+	printk("Grasshopper: %lu PWMs\n", ARRAY_SIZE(pwm_led));
+#endif
+	
+	// LEDs
+	for(i=0;i<ARRAY_SIZE(grasshopper_led);i++)
+		at32_select_gpio(grasshopper_led[i].gpio, AT32_GPIOF_OUTPUT);
+
+	printk("Grasshopper: %lu Leds\n", ARRAY_SIZE(grasshopper_led));
+	platform_device_register(&grasshopper_led_dev);
+
+	// Button(s)
+	for(i=0;i<grasshopper_buttons_data.nbuttons;i++)
+		at32_select_gpio(grasshopper_buttons_data.buttons[i].gpio, 
+				AT32_GPIOF_DEGLITCH);
+	platform_device_register(&grasshopper_buttons);
+
+	// Ethernet
+	set_hw_addr(at32_add_device_eth(0, &eth_data[0]));
+
+	// I2C / TWI
+#ifdef CONFIG_I2C
+	/* all these i2c/smbus pins should have external pullups for
+	 * open-drain sharing among all I2C devices.  SDA and SCL do;
+	 * PB28/EXTINT3 doesn't; it should be SMBALERT# (for PMBus),
+	 * but it's not available off-board.
+	 */
+	at32_select_periph(GPIO_PIOB_BASE, 1 << 28, 0, AT32_GPIOF_PULLUP);
+	at32_select_gpio(i2c_gpio_data.sda_pin,
+		AT32_GPIOF_MULTIDRV | AT32_GPIOF_OUTPUT | AT32_GPIOF_HIGH);
+	at32_select_gpio(i2c_gpio_data.scl_pin,
+		AT32_GPIOF_MULTIDRV | AT32_GPIOF_OUTPUT | AT32_GPIOF_HIGH);
+	platform_device_register(&i2c_gpio_device);
+	i2c_register_board_info(0, i2c_info, ARRAY_SIZE(i2c_info));
+#endif
+
+	// USB
+//	at32_add_device_usba(0, NULL);
+
+	// MCI
+#if defined(CONFIG_MMC_ATMELMCI) || defined(CONFIG_MMC_ATMELMCI_MODULE)
+	at32_add_device_mci(0, &mci0_data); /* MMC/SD */
+#endif
+
+	// PS/2
+#ifdef CONFIG_SERIO_AT32PSIF
+	at32_add_device_psif(0);
+#endif
+	
+	// AC97
+//	at32_select_gpio(GPIO_AC_OUTPUT_SELECT,AT32_GPIOF_OUTPUT);
+//	at32_select_gpio(GPIO_AC_RESET,AT32_GPIOF_OUTPUT);
+//	at32_add_device_ac97c(0);
+
+	return 0;
+}
+postcore_initcall(grasshopper_init);
